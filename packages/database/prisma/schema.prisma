model pulse_aggregates {
  id        BigInt  @id @default(autoincrement())
  bucket    Int
  period    Int
  type      String  @db.VarChar(255)
  key       String
  key_hash  String  @default(dbgenerated("(md5(key))::uuid")) @db.Uuid
  aggregate String  @db.VarChar(255)
